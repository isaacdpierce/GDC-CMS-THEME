// PAGINATION
.boost-pfs-filter-bottom-pagination-default {
  text-align: right;
  padding-top: 25px;
  padding-bottom: 25px;

  > ul {
    margin: 0;
    text-align: center;
    padding-left: 0;

    > li {
      display: inline-block;
      list-style: none;
      width: 40px;
      height: 40px;
      line-height: 40px;
      vertical-align: middle;

      @include border-radius(100%);

      @media (max-width: #{$breakpointMobile}){
        width: 32px;
        height: 32px;
        line-height: 32px;
      }

      &:first-child,
      &:last-child {
        border: 1px solid #3D4246;
        font-size: 20px;

        &.boost-pfs-filter-pagination-disabled {
          @include opacity(0.5);
        }
        &:not(.boost-pfs-filter-pagination-disabled){
          &:hover {
            background: #3D4246;
            a {
              color: #fff;
            }
          }
        }
      }
      &:first-child {
        margin-right: 5px;
      }
      &:last-child {
        margin-left: 5px;
      }

      a,
      span {
        color: #000;
        padding: 0;
        margin: 0 6px;
        font-weight: 400;
        text-decoration: none;
      }
      span.boost-pfs-filter-pagination-active {
        text-shadow: 0 0 1px #000;
      }
      a:hover {
        text-shadow: 0 0 1px #000;
      }
    }
  }
}
