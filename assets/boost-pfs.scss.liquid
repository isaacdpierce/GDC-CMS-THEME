/* SORT BY CUSTOM UL LI */
.boost-pfs-filter-default-toolbar {
  * {
    -moz-box-sizing: border-box;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
  }

    .boost-pfs-filter-top-sorting {
        button {
            display: inline-block;
        }
    }
}

.boost-pfs-filter-top-sorting button {
  margin-right: 0;
}

.boost-pfs-filter-custom-sorting {

  button {
    display: block;
    cursor: pointer;
    text-transform: capitalize;
    padding-left: 5px;
    padding-right: 20px;
    margin-bottom: 0;
    background: none;
    border: none;
    color: inherit;
    outline: none;

    > span {
      position: relative;
      line-height: 55px;

      &:after {
        top: 50%;
        right: 0;
        left: auto;
        right: -20px;
        position: absolute;
        margin-top: -2px;
        content: "";
        border-right: 1px solid #222;
        border-bottom: 1px solid #222;
        display: inline-block;
        @include transition(all 100ms ease-in-out);
        @include transform(rotate(45deg));
        width: 7px;
        height: 7px;
        display: inline-block;
        text-transform: none;
        -webkit-font-smoothing: antialiased;
        -moz-osx-font-smoothing: grayscale;
        font-size: 11px;
        margin-left: 10px;
        vertical-align: top;
        margin-top: -5px;
        white-space: nowrap;
        overflow: hidden;
        line-height: inherit;
      }
      >span {
        overflow: hidden;
        white-space: nowrap;
        line-height: normal;
      }
    }
  }
  &.boost-pfs-filter-sort-active {
    button>span:after {
      @include transform(rotate(-135deg));
      margin-top: -2px;
    }
  }
}

.boost-pfs-filter-custom-sorting {
  position: relative;

  .boost-pfs-filter-filter-dropdown {
    list-style: none;
    position: absolute;
    background: #fff;
    border: 1px solid #eee;
    padding: 0px;
    z-index: 999;
    text-align: left;
    width: 250px;
    right: 0;
    top: 100%;
    margin: 0;
    display: none;

    li {
      padding: 10px 20px;
      list-style: none;

      &:first-child {
        padding-top: 20px;
      }
      &:last-child {
        padding-bottom: 20px;
      }

      a {
        border-bottom: 1px solid transparent;
        display: inline-block;
        line-height: 1.2;

        @media screen and (min-width: 1200px) {
          &:hover,
          &.boost-pfs-filter-sort-item-active {
            font-weight: 400;
            text-decoration: underline;
            text-underline-position: under;
          }
        }
      }
    }
  }
}


